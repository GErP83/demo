<div class="post-card">
    
    <a href="{{permalink}}" target="">
        <img src="{{imageUrl}}">
    </a>

    <div class="post-meta">

        <time datetime="{{publication.html}}">
            {{publication.html}}
        </time>
        |
        {{#authors}}
            <a href="{{permalink}}">{{title}}</a>
        {{/authors}}
       
    </div>

    <div class="post-title">
        <a href="{{permalink}}" target="">{{title}} </a>
    </div>
    
</div>